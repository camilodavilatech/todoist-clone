---
import Sideabar from "@/components/Sideabar.astro";
import Layout from "./Layout.astro";
import Header from "@/components/Header.astro";
import BotAgregar from "@/components/BotAgregar.astro";

type Props = {
  title: string;
};

const { title } = Astro.props;
---

<Layout title={title}>
  <Sideabar />
  <main class="hero">
    <Header />
    <slot />
    <BotAgregar />
  </main>
</Layout>
